/* pages/meal/index/index.wxss */
.top{
  width: 96%;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 2%;
  background: #fff;
  display:flex;
  justify-content: space-between;
  border-bottom: 1px solid #eee;
}
.top>view:first-child{
  flex: 1;
  display:flex;
}
.top>view:first-child>image{
  width: 25rpx;
  height: 30rpx;
  margin: 10rpx 5rpx;
}
.top>view:first-child>view{
  padding-left: 10rpx;
  font-size: 26rpx;
  color: #666;
}
.top>view:first-child>view>text{
  font-size: 26rpx;
  color: #333;
}
.top>navigator{
  display:flex;
}
.top>navigator>image{
  width: 30rpx;
  height: 30rpx;
  margin: 10rpx;
}
.top>navigator>view{
  padding-left: 5rpx;
  font-size: 26rpx;
  color: #309EFD;
}
/*搜索*/
.index-sousuo{
	width: 100%;
	height: 90rpx;
	position: relative;
}
.index-sousuo>view{
	width: 96%;
	height: 56rpx;
	background-color: #fff;
	line-height: 56rpx;
	margin: 17rpx 2%;
	border-radius: 10rpx;
	text-align: center;
	text-indent: 2em;
	font-size: 26rpx;
	color: #b2b2b2;
}
.index-sousuo>image{
	position: absolute;
	width: 25rpx;
	height: 25rpx;
	top: 33rpx;
	left: 46%;
}
/*轮播  */
.banner{
  width: 750rpx;
  height: 300rpx;
}
.banner_img{
  width: 750rpx;
  height: 300rpx;
  display: block;
}
/*为你优选  */
.choice_wrap{
  position: relative;
  width: 750rpx;
  height: 88rpx;
}
.choice_img{
  width: 750rpx;
  height: 88rpx;
  display: block;
}
.choice_txt{
  color: #0d86f0;
  font-size: 32rpx;
  line-height: 88rpx;
  height: 88rpx;
  width: 750rpx;
  text-align: center;
  position: absolute;
  left: 0;
  top: 0;
  letter-spacing: 5rpx;
}

.choice_content{
  width: 100%;
}
.choice_item{
  width: 331rpx;
  height: 100rpx;
  padding: 10rpx;
  border-radius: 10rpx;
  margin-left: 16rpx;
  margin-bottom: 16rpx;
}
.goods_img{
  width: 100rpx;
  height: 100rpx;
  border-radius: 10rpx;
}
.choice_l{
  width: 221rpx;
  height: 100rpx;
  margin-left: 10rpx;
}
.goods_name{
  width: 221rpx;
  color: #666;
  font-size: 26rpx;
  line-height: 54rpx;
  height: 54rpx;
}
.go{
  width: 221rpx;
  height: 46rpx;
  line-height: 46rpx;
}
.go view{
  font-size: 30rpx;
  color: #666;
}
.arrow_r{
  width: 14rpx;
  height: 22rpx;
  padding: 12rpx 8rpx;
}
/*广告  */
.ad_wrap{
  margin: 0 15rpx;
  margin-bottom: 16rpx;
  height: 140rpx;
}
.ad{
  width: 718rpx;
  height: 140rpx;
  margin: 0 auto;
  display: block;
}
/*美食美客  */
.cate_wrap{
  position: relative;
  width: 750rpx;
  height: 90rpx;
}
.cate_img{
  width: 750rpx;
  height: 90rpx;
  display: block;
}
.cate_txt{
  color: #000;
  font-size: 30rpx;
  line-height: 90rpx;
  height: 90rpx;
  width: 750rpx;
  text-align: center;
  position: absolute;
  left: 0;
  top: 0;
}
.tab_wrap{
  width: 100%;
  height: 80rpx;
  display: flex;
  background: #fff;
  border-top: 1rpx solid #ccc;
  border-bottom: 1rpx solid #ccc;
}
.tab_item{
  flex: 1;
  text-align: center;
  line-height: 80rpx;
  color: #666;
  font-size: 26rpx;
  position: relative;
}
.arrow2{
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  top: 25rpx;
  right: 30rpx;
}
.active{
  color: #309efd;
}
/*商品列表  */
.list{
  padding: 0 15rpx;
  margin-bottom: 15rpx;
}
.item_wrap{
  padding: 10rpx;
  background: #fff;
  margin-top: 15rpx;
}
.list_item{
  border: 1rpx solid #cdcdcd;
  width: 688rpx;
  height: 200rpx;
  padding: 5rpx;
}
.list_img{
  width: 200rpx;
  height: 200rpx;
}
.list_l{
  margin-left: 30rpx;
  width: 450rpx;
}
.shop_name{
  color: #333;
  font-size: 30rpx;
  line-height: 54rpx;
  height: 54rpx;
}
.shop_goods{
  font-size: 24rpx;
  color: #666;
  line-height: 40rpx;
  height: 80rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
}
.list_r_b{
  height: 50rpx;
}
.sales{
  color: #656565;
  font-size: 24rpx;
  height: 35rpx;
  margin-top:15rpx; 
}
.price{
  color: #666;
  font-size: 30rpx;
}
.money{
  color: #ff0000;
  font-size: 40rpx;
}

/*  加载更多   */
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 10rpx auto;
  line-height: 60rpx;
  font-size: 28rpx;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}