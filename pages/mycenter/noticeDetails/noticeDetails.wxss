/* pages/mycenter/noticeDetails/noticeDetails.wxss */
@import '../../../wxParse/wxParse.wxss';
page{
	width: 100%;
	height: 100%;
	background: #fff;
}
.notice{
	width: 96%;
	height: auto;
	padding: 15rpx 2%;
}
.notice>view:first-child{
	width: 100%;
	height: 60rpx;
	line-height: 60rpx;
	text-align: center;
	font-size: 34rpx;
	color: #333;
}
.notice>view:nth-child(2){
	font-size: 28rpx;
	color: #666;
	text-align: center;
}
.notice>view:last-child{
	font-size: 30rpx;
	color: #333;
	line-height: 46rpx;
	margin-top: 15rpx;
}